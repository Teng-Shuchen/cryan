#wrap .page5 .page3-box .row .box-item img {
  width: 100%;
  min-height: 220px;
  max-height: 220px;
}
#wrap .page5 .page3-box .row .box-item .item-zhez {
  position: absolute;
  text-align: left;
  display: flex;
  flex-flow: column;
  justify-content: center;
  color: white;
  padding: 20px;
  right: 0;
  bottom: 0;
  left: 0;
  top: 0;
  background-color: rgba(0, 0, 0, 0.3);
